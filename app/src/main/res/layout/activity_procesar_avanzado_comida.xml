<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/primaryColor"
    android:layout_height="match_parent"
    tools:context=".avanzado.procesarAvanzadoComida">
 <TextView
        android:id="@+id/tvResultado"
        android:text="Resultado"
        style="@style/styleTitulo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01" />

    <TextView
        android:id="@+id/textView7"
        android:text="SecciÃ³n:"
        style="@style/styleSubtitulo"
        android:background="@color/primaryLightColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvResultado"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/textView11"
        style="@style/styleSubtitulo"
        android:text="Nivel:"
        android:background="@color/primaryLightColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvSeccion"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="0.05" />

    <TextView
        android:id="@+id/textView13"
        android:text="Puntaje:"
        style="@style/styleSubtitulo"
        android:background="@color/primaryLightColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvNivel"
        app:layout_constraintVertical_bias="0.08" />

    <TextView
        android:id="@+id/textView16"
        android:text="Respuestas:"
        style="@style/styleSubtitulo"
        android:background="@color/primaryLightColor"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvpuntaje"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/tvpuntaje"
        android:text="Puntaje"
        android:textColor="#FD0500"
        android:textStyle="bold"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.275"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/textView17"
        android:text="Correcta: "
        android:textColor="#FD0500"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/textView19"
        android:text="Incorrecta: "
        android:textColor="#FD0500"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        app:layout_constraintVertical_bias="0.02"
        app:layout_constraintHorizontal_bias="0.415" />

    <TextView
        android:id="@+id/textView15"
        android:text=" puntos de 30"
        android:textColor="#FD0500"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvpuntaje"
        app:layout_constraintTop_toBottomOf="@+id/textView13"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.02" />

    <TextView
        android:id="@+id/tvcorrecta"
        android:text="correcta"
        android:textColor="#FD0500"
        android:textStyle="bold"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView17"
        app:layout_constraintTop_toBottomOf="@+id/textView16"
        app:layout_constraintVertical_bias="0.02"
        app:layout_constraintHorizontal_bias="0.0" />

    <TextView
        android:id="@+id/tvincorrecta"
        android:text="incorrecta"
        android:textColor="#FD0500"
        android:textStyle="bold"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView19"
        app:layout_constraintTop_toBottomOf="@+id/tvcorrecta"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintVertical_bias="0.02"/>

    <TextView
        android:id="@+id/tvNivel"
        android:text="Avanzado"
        android:textColor="#0D30F6"
        style="@style/styleLabel"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintVertical_bias="0.064" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="70dp"
        android:layout_height="70dp"
        app:layout_constraintBottom_toTopOf="@+id/textView11"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/comida_2" />

    <TextView
        android:id="@+id/tvSeccion"
        style="@style/styleLabel"
        android:text="Comida"
        android:textColor="#0D30F6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.3"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/textView7"
        app:layout_constraintVertical_bias="0.05" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintBottom_toTopOf="@+id/textView13"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11"
        app:layout_constraintHorizontal_bias="0.35"
        app:layout_constraintVertical_bias="0.5"
        app:srcCompat="@drawable/tres" />

    <Button
        android:id="@+id/btnOk"
        style="@style/styleButton"
        android:text="OK"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.126"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvincorrecta"
        app:layout_constraintVertical_bias="0.075" />

    <Button
        android:id="@+id/irRanking"
        style="@style/styleButton"

        android:layout_marginStart="104dp"
        android:text="Ir Ranking"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnOk"
        app:layout_constraintTop_toBottomOf="@+id/tvincorrecta"
        app:layout_constraintVertical_bias="0.075" />
</androidx.constraintlayout.widget.ConstraintLayout>